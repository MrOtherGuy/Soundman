{
  "description": "Adds context menu to control playback in tabs",
	"author": "MrOtherGuy",
  "manifest_version": 2,
  "name": "Soundman",
  "permissions": ["tabs","<all_urls>","menus","storage"],
  "version": "0.0.9",
	"background": {
		"scripts": ["background-script.js"]
	},
	"options_ui": {
      "page": "options/options.html",
      "browser_style": true
  },
	"applications": {
  "gecko": {
    "id": "soundman@example.com",
		"strict_min_version": "61.0"
  }
}
}